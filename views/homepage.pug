doctype html
html
  head
    title= title
  body
    h1= title

    a(href='/update-cobj') Add to this table

    if records.length
      table(border='1')
        thead
          tr
            th ID
            th Name
            th Description
            th Type
        tbody
          each record in records
            tr
              td= record.id
              td= record.properties.name
              td= record.properties.description
              td= record.properties.type
    else
      p No records found.
